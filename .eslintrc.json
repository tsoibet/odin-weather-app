{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-use-before-define": "off",
        "no-param-reassign": "warn",
        "no-restricted-syntax": "off",
        "guard-for-in": "off",
        "max-len": "off"
    }
}
